{
    "name": "felicity-php/felicity-translate",
    "version": "1.1.0",
    "description": "Translation functionality for PHP",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "felicity\\translate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "roave/security-advisories": "dev-master",
        "felicity-php/felicity-config": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "felicity-php/felicity-twig": "^1.0"
    },
    "extra": {
        "bootstrap": [
            {
                "type": "classMethod",
                "class": "felicity\\translate\\Bootstrap",
                "method": "run"
            }
        ]
    }
}
